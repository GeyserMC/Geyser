    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-GeyserStandaloneBootstrap<bootstrap-standalone>",
            "request": "launch",
            "mainClass": "org.geysermc.platform.standalone.GeyserStandaloneBootstrap",
            "projectName": "bootstrap-standalone",
            "cwd": "${workspaceFolder}",
            "args": "-Xms1G"
        }
    ]
}
